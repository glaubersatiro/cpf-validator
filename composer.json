{
    "name": "glaubersatiro/cpf-validator",
    "description": "Biblioteca de validação de números CPF (Cadastro de Pessoas Físicas) do Brasil / Library for validating Brazilian CPF numbers",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "glaubersatiro",
            "email": "glauber@satiro.dev"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "GlauberSatiro\\": "src/"
        }
    },
    "autoload-dev": {
        "prs-4": {
            "GlauberSatiro\\Validator\\Cpf\\Tests\\": "tests/"
        }
    },
    "require-dev": {
